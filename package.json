{
  "name": "@dikac/t-map",
  "version": "0.8.5",
  "description": "Typescript map",
  "dependencies": {
    "@dikac/t-object": "^0.9.22",
    "@types/node": "^12.12.3",
    "dist-mover": "0.0.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "jasmine": "^3.4.0"
  },
  "scripts": {
    "spec-run": "jasmine --config=./jasmine.json",
    "spec": "npm-run-all spec-compile spec-run",
    "spec-compile": "npm-run-all --parallel spec-compile-script ",
    "spec-compile-watch": "npm-run-all --parallel spec-compile-watch-script",
    "spec-compile-script": "tsc -p tsconfig-spec.json",
    "spec-compile-watch-script": "tsc -w -p tsconfig-spec.json",
    "compile": "tsc",
    "compile-watch": "tsc -w",
    "postinstall": "npm run production",
    "production": "dist-mover"
  },
  "files": [
    "dist/",
    "LICENSE",
    "Readme.md",
    "tsconfig.json"
  ],
  "author": "",
  "license": "MIT"
}
